{
  "id": "dcos-blue-app",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "chrisciborowski/dcos-blue-app",
      "network": "BRIDGE",
      "portMappings": [
        { "hostPort": 0, "containerPort": 5000, "servicePort": 10002 }
      ],
      "forcePullImage":true
    }
  },
  "instances": 1,
  "cpus": 0.1,
  "mem": 65,
  "labels":{
    "HAPROXY_GROUP":"external"
  }
}
